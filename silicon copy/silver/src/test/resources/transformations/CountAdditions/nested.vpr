method main()
{
    var x:Int
    assert(1 + 2 + 3 + 0 == 6)
    while(1 + 2 < 3) {
        inhale 6 + 3 == 5 + 4
        assume 3 + 2 == 1 + 4
        if(3 + 4 > 1) {
            x := 1 + 3
        }
    }
}
