field f: Int
field next: Ref

predicate valid(self: Ref) {
    acc(self.f)
}

method read(a: Ref)
    requires acc(valid(a), write)
    ensures acc(valid(a), write)
{
    assert perm(a.f) == none /* OK - perm(e.f) does not require e != null */
}
