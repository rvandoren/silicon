import "list.vpr"
import "account.vpr"

/****************************************
 * Client
 ****************************************/

method demo(a: Ref, l: Ref)
  requires acc(account(a)) && acc(list(l))
{
  var tmp: Int
  tmp := length(l)
  deposit(a, 200)
  assert tmp == length(l)
}
